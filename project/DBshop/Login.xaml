<Window x:Class="DBshop.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBshop"
        mc:Ignorable="d"
        Title="Login" Height="240" Width="150" Closing="Window_Closing" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="tb1" HorizontalAlignment="Center" Margin="0,17,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="30" FontSize="14" Text="admin"/>
        <TextBox x:Name="tb2" HorizontalAlignment="Center" Margin="0,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="30" FontSize="14" Text="admin"/>
        <Button x:Name="logButton" Content="Login" HorizontalAlignment="Center" Margin="0,146,0,0" VerticalAlignment="Top" Width="120" Height="30" Click="logButton_Click"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="9,55,0,0" VerticalAlignment="Top"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="9,-3,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
